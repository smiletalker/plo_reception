<!--登录-->
<section class="login">
    <div class="left">
        <form id="myLoginForm">
            <div class="loginTitle">
                <span>客户登录</span>
                <br/>
                <span>如果您有账户，请使用您的电子邮件地址登录。</span>
            </div>
            <!--输入框，电子邮件、密码-->
            <div class="please">
                <div class="pleaseCss">
                    <label for="myEmail">电子邮件</label>
                    <input type="text" id="myEmail" name="userEmail" onblur="checkEmail(this,'请输入您的电子邮件地址')" placeholder="请输入您的电子邮件地址">
                    <span id="emailTips"></span>
                </div>
                <div class="pleaseCss">
                    <label for="myPwd">密码</label>
                    <input type="password" id="myPwd" name="userPwd" onblur="checkPwd(this,'请输入您的密码')" placeholder="请输入您的密码">
                    <span id="pwdTips"></span>
                </div>
            </div>
            <!--记住我-->
            <div class="remember">
                <input type="checkbox" id="box" name="box">
                <label for="box">记住我</label>
                <span title="即使您未登录，也请选中'记住我'以访问此计算机上的购物车。">这是什么？</span>
            </div>
            <!--提交按钮-->
            <div class="subBut">
                <button type="button" class="mySubmit butHoverCss" onclick="mySubmit()">提交</button>
            </div>
            <div class="forgetPwd">
                <button type="button" class="btn btn-default" data-toggle="modal" data-target="#myModal">
                    忘记密码了吗？
                </button>
                <!--<button></button>-->
            </div>
        </form>
    </div>
    <div class="right">
        <div class="loginTitle">
            <span>新客户</span>
            <br/>
