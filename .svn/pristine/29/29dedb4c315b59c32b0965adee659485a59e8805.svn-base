.butHoverCss{
    --color: #fff;
    --hover: rgb(0,140,168);
}
.butHoverCss:hover, .butHoverCss:focus {
    border-color: var(--hover);
    color: #fff;
    box-shadow: inset 20em 0 0 0 var(--hover);
}
.butHoverCss{
    color: var(--color);
    -webkit-transition: 0.25s;
    transition: 0.8s;
}
