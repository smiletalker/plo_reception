}
.butHoverCss{
    --color: #fff;
    --hover: rgb(0,140,168);
}
.butHoverCss:hover, .butHoverCss:focus {
    border-color: var(--hover);
    color: #fff;
    box-shadow: inset 10em 0 0 0 var(--hover);
}
.butHoverCss{
    color: var(--color);
    -webkit-transition: 0.25s;
    transition: 1s;
}

/*复选框地址*/
input[type="radio"]{
    -webkit-appearance:none;
    width: 13px;
    height: 13px;
    /*float: left;*/
    border:1px solid black;
    background-color: rgb(255, 255, 255);
    margin-top: 3px;
    margin-left: 8px;
    position: relative;
    cursor: pointer;

}
input[type="radio"]:focus{
    outline: none;
}
input[type="radio"]+label{
    cursor: pointer;
}
input[type="radio"]:checked::after{
    content: '\2714';
    color: rgb(0,140,168);
    font-weight: bold;
    font-size: 12px;
    border: none;
    position: absolute;
    top: -3px;
    left: 0;
}
