}
.smallImgD>ul>li>a{
    color: black;
    font-size: 14px;
    text-decoration: none;
}

hr{
    margin-top: 10%;
}
.smallImg:hover {
    animation: mySmallImg .4s linear;
    border-radius: 10%;
}
@-webkit-keyframes mySmallImg {
    0%{

        margin-left: 0;

    }

    50%{
        /*width: 95%;*/
        transform: scale(1.2);
    }

    100%{

        margin-left: 0;
    }
}

