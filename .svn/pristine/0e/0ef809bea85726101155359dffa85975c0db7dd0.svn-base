    opacity: 0;
}
.smallImgD{
    width: 10%;
    height: 55%;
    float: left;
    margin-top: 13%;
    margin-left: 3%;
}
.smallImgD1{
    margin-left: 20%;
}
.smallImg{
    width: 100%;
    height: 35%;
    transition: all .3s linear;


}
.smallImgD>ul>li{
