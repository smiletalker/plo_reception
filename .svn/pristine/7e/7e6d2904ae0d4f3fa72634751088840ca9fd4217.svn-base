*{
    margin: 0;
    padding: 0;
    font-family: 微软雅黑;
}
/*页头*/
header{
    /*background: url("../images/index/HomePgMarquee_1.jpg");*/
    background-size: 100% 100%;
    width: 100%;
    height: 655px;
    position: absolute;
    /*outline: 1px solid green;*/
    /*z-index: 99;*/
}
#navD{
    width: 100%;
    height: 10%;
    position: relative;
    background: white;
}
/*logo图*/
#logoW{
    width: 12%;
    margin-top: 0.5%;
    margin-left: 5%;
    position: relative;
    z-index: 2;
}
/*导航*/
nav{
    width: 80%;
    height: 10%;
    position: absolute;
    top: 4.8%;
    left: 20%;
    z-index: 2;
    /*outline: 1px solid red;*/
}
nav>ul>li{
    float: left;
    margin-left: 9%;
    list-style: none;
    margin-top: 2%;
}
nav>ul>li>a{
    color: black;
    text-decoration: none;
    font-size: 14px;
}
.navLi1{
    margin-left: 27%;
}
nav>span>a{
    color: black;
    text-decoration: none;
    font-size: 14px;
}
nav>span{
    margin-left: 2%;
}
.navSpan1{
    display: inline-block;
    margin-left: 20%;
    margin-top: 1%;

}
#navSpan2{
    position: absolute;
    font-size: 30px;
    color: black;
    top: 5%;
    left: 85%;
    cursor: pointer;
}
#navMoveSd{
    height: 5px;
    width: 30px;
    background: rgb(0,140,168);
    margin-left: 27%;
    margin-top: 1%;
    opacity: 0;
    transition: all .2s linear;
}


/*商店导航DIV*/
#shopD{
    width: 100%;
    height: 80%;
    background: white;
    box-shadow: #999999  5px 5px 10px 1px;
    position: absolute;
    top: 0;
    left: -100%;
    z-index: 1;
    transition: all .4s linear;
    opacity: 0;
}
.smallImgD{
    width: 10%;
    height: 55%;
    float: left;
    margin-top: 13%;
    margin-left: 3%;
}
.smallImgD1{
    margin-left: 20%;
}
.smallImg{
    width: 100%;
    height: 35%;
    transition: all .3s linear;


}
.smallImgD>ul>li{
    list-style: none;
    margin-top: 4%;
    transition: all .3s linear;
}
.smallImgD>ul>li>a{
    color: black;
    font-size: 14px;
    text-decoration: none;
}

hr{
    margin-top: 10%;
}
.smallImg:hover {
    animation: mySmallImg .4s linear;
    border-radius: 10%;
}
@-webkit-keyframes mySmallImg {
    0%{

        margin-left: 0;

    }

    50%{
        /*width: 95%;*/
        transform: scale(1.2);
    }

    100%{

        margin-left: 0;
    }
}

/*社区导航DIV*/
#communityD{
    width: 100%;
    height: 70%;
    background: white;
    box-shadow: #999999  5px 5px 10px 1px;
    position: absolute;
    top: 0;
    left: -100%;
    z-index: 1;
    transition: all .4s linear;
    opacity: 0;
}
.midD{
    width: 13%;
    height: 40%;
    float: left;
    margin-top: 15%;

}
.midD1{
    margin-left: 16%;
}
.communityImg{
    width: 90%;
    height: 80%;
    transition: all .3s;
}
.midD>p{
    transition: all .3s linear;
}
.midD>p>a{
    font-size: 14px;
    text-decoration: none;
    color: black;
}
.communityImg:hover{
    animation: mySmallImg2 .4s linear;
    border-radius: 10%;
}
@-webkit-keyframes mySmallImg2 {
    0%{

        margin-left: 0;

    }

    50%{
        /*width: 95%;*/
        transform: scale(.9);
    }

    100%{

        margin-left: 0;
    }
}


/*支持导航DIV*/
#supportD{
    width: 100%;
    height: 70%;
    background: white;
    box-shadow: #999999  5px 5px 10px 1px;
    position: absolute;
    top: 0;
    /*left: -100%;*/
    z-index: 1;
    transition: all .4s linear;
    opacity: 0;
}
.bigD{
    width: 30%;
    height: 55%;
    margin-left: 35%;
    margin-top: 13%;
}
.bigD>img{
    width: 55%;
    height: 90%;
    float: left;
    transition: all .3s linear;
}
.bigD>ul{
    float: left;
    margin-left: 5%;
    margin-top: 7%;
    width: 30%;
}
.bigD>ul>li{
    list-style: none;
    margin-top: 12%;
    transition: all .3s linear;
}
.bigD>ul>li>a{
    color: black;
    text-decoration: none;
    font-size: 14px;
}
.bigD>img:hover{
    animation: mySmallImg2 .4s linear;
    border-radius: 10%;
}
/*搜索框*/
#searchIn{
    width: 30%;
    position: absolute;
    top: -200%;
    left: 61%;
    height: 40px;
    opacity: 0;
    transition: all .3s linear;
}
/*页头中间内容*/
#headConD{
    width: 30%;
    height: 40%;
    outline: 1px solid red;
    position: absolute;
    top: 40%;
    left: 10%;
    color: white;
}
#headConD>h1{
    font-size: 50px;
}