body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, form, fieldset, legend, button, input, textarea, th, td{
    margin:0; padding:0;
}
@font-face {
    font-family: 'iconfont';
    src: url('../font/iconfont.eot');
    src: url('../font/iconfont.eot?#iefix') format('embedded-opentype'),
    url('../font/iconfont.woff') format('woff'),
    url('../font/iconfont.ttf') format('truetype'),
    url('../font/iconfont.svg#iconfont') format('svg');
}
.iconfont{
    font-family:"iconfont" !important;
    font-size:16px;font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
}
.blog_header{
    height: 450px;
    width: 100%;
    background-image: url("../images/person/AlexPAthleteHeader.jpg");
    background-size:cover;
    overflow: hidden;
}
.blog_header_word{
    margin-top: 120px;
    margin-left: 100px;
}

.blog_header_word p:nth-child(1){
    color: white;
    letter-spacing: 2px;
}
.blog_header_word h1{
    color: white;
    font-size: 80px;
    font-weight: lighter;
}
.blog_header_word p:nth-last-child(1){
    height: 3px;
    background-color: #008ca8;
    width: 150px;
}

.blog_header_chio{
    margin-top: 5%;
    margin-left: 20%;
    overflow: hidden;
}
.person{
    height:290px;
    width: 290px;
    border-radius:100%;
    overflow: hidden;
    background-image: url("../images/person/Andres_Marin_profile.jpg");
}
.person_all{
    float: left;
    margin-top: 50px;
    margin-left: 100px;
}
.person_inner{
    height:290px;
    width: 290px;
    margin-top: -290px;
    transition: all .3s linear;
    border-radius:100%;
    background-image: url("../images/person/Andres_Marin_Square.jpg");
}
.person_title{
    clear: both;
    width: 200px;
    margin-left: 55px;
}
.person_title p{
    margin-left: 55px;
    padding-top: 20px;
    padding-bottom: 10px;
}
.person_title hr{
    height: 2px;
    background-color: #008ca8;
}

.person:hover .person_inner{
   margin-top:0px;
}
