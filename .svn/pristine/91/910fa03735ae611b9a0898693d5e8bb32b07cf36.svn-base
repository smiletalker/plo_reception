}

/*按钮*/
.buttonD{
    width: 100px;
    height: 40px;
    background: black;
    border: none;
    margin-top: 6%;
    font-size: 12px;
    margin-left: 208%;
    cursor: pointer;
}
.butHoverCss{
    --color: #fff;
    --hover: rgb(0,140,168);
}
.butHoverCss:hover, .butHoverCss:focus {
    border-color: var(--hover);
    color: #fff;
    box-shadow: inset 17em 0 0 0 var(--hover);
}
.butHoverCss{
    color: var(--color);
    -webkit-transition: 0.25s;
    transition: 0.8s;
}

#headboD{
    width: 60%;
    height: 7%;
    margin-left: 20%;
    margin-top: 33%;
    background: black;
}
#headboD>ul{
    width: 100%;
